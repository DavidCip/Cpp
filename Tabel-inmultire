
#include <iostream>
using namespace std;

int main()
{
    int numar, inmultire;
    cout << "Introduceti numarul" << endl;
    cin >> numar;

    for (inmultire=1;inmultire<=10;inmultire++) 
        {   

        cout << numar << " * " << inmultire << " = " << (numar * inmultire) << endl;
        
        }
}


